<%= form_for attraction do |f| %>
  <%= render 'shared/errors', messages: attraction.errors.full_messages %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :media %>
  <%= f.text_field :media %>

  <%= f.label :day %>
  <%= f.datetime_select :day %>

  <%= f.submit %>
<% end %>
